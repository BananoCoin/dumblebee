{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "require-await": "error",
    "max-len": [
      "error",
      {
        "code": 160
      }
    ]
  }
}
